{
    "manifest_version": 3,
    "name": "UP",
    "description": "Тайная разработка Мистериона ради спасения простых смертных",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon.png"
    },
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "icons":{
        "16": "images/icon.png",
        "128": "images/icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [ "data.json" ],
            "matches": ["<all_urls>"]
        }
    ]
}